function lazypush() {
    git add -A
    git commit -a -m "$1"
    git push
}

function lazyamend() {
  git stash
  git rebase master
  git stash pop
  git add -A
  git commit --amend
}

alias gits="git status"
alias gitl="git log"
alias gitb="git branch"
alias gpush="git push"
alias gpull="git pull"
