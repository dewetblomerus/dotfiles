function lazypush() {
    git add -A
    git commit -a -m "$1"
    git push
}

function lazyamend() {
  git add -A
  git commit --amend
}

function gfile() {
  git diff-tree --no-commit-id --name-only -r "$1"
}

alias gits="git status"
alias gitl="git log"
alias gitb="git branch"
alias gpush="git push"
alias gpull="git pull"
